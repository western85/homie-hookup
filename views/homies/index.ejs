<% layout('layouts/boilerplate')%>

    <body>

        <h1>All Homies</h1>
        <a href="homies/new">Add a Homie</a>
        <ul>
            <% for (let homie of homies) { %>
                <li>
                    <a href="/homies/<%= homie._id %>">
                        <%= homie.name %>
                    </a>
                </li>
                <% } %>
        </ul>
    </body>